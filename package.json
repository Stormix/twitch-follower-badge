{
	"name": "twitch-stalker",
	"version": "0.0.0",
	"scripts": {
		"prepare": "husky",
		"lint": "turbo run lint --parallel",
		"format": "turbo run format --parallel",
		"extension:dev": "pnpm --filter @twitch-stalker/extension dev",
		"db:up": "docker compose up -d",
		"db:down": "docker compose down"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"husky": "^9.1.7",
		"turbo": "^2.3.3"
	},
	"packageManager": "pnpm@10.4.0"
}
